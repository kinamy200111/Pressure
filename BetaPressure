-- GUI Elements Array for F1 Toggle
local AllGUIElements = {
    MainUIsystem, MainUIsystem_Border, Line, Line_Border, Line1, Line1_Border,
    RATHUBlogo, Line2, Line2_Border, HOMEtabB, HOMEtabB_Text, TabVisualsB, TabVisualsB_Text,
    TabHomeContainer1, WorkGames1, PressureWORK, TabVisualsContainer,
    Angler, Angler_Bg, Angler_IndBorder, Angler_Ind, Angler_Label,
    Froger, Froger_Bg, Froger_IndBorder, Froger_Ind, Froger_Label,
    Pinkie, Pinkie_Bg, Pinkie_IndBorder, Pinkie_Ind, Pinkie_Label,
    Blitz, Blitz_Bg, Blitz_IndBorder, Blitz_Ind, Blitz_Label,
    Pandemonium, Pandemonium_Bg, Pandemonium_IndBorder, Pandemonium_Ind, Pandemonium_Label,
    Harbrinder, Harbrinder_Bg, Harbrinder_IndBorder, Harbrinder_Ind, Harbrinder_Label,
    A60, A60_Bg, A60_IndBorder, A60_Ind, A60_Label,
    Painter, Painter_Bg, Painter_IndBorder, Painter_Ind, Painter_Label,
    Keycard, Keycard_Bg, Keycard_IndBorder, Keycard_Ind, Keycard_Label,
    Line5, Line5_Border, EspText, ESPitems, MiscTAB, MiscTAB_Text,
    SettingsTAB, SettingsTAB_Text, MiscTABcontainer1,
    AutoHideGUI, AutoHideGUI_Inner, AutoHideGUI_Label,
    WatermarkButton, WatermarkButton_Inner, WatermarkButton_Label
}

local function ToggleGUI()
    Settings.guiVisible = not Settings.guiVisible
    for _, element in ipairs(AllGUIElements) do
        if element and element.Visible ~= nil then
            if Settings.guiVisible then
                if element == TabHomeContainer1 or element == TabVisualsContainer or element == MiscTABcontainer1 then
                    element.Visible = false
                elseif element == WorkGames1 or element == PressureWORK then
                    element.Visible = false
                elseif element == Angler or element == Froger or element == Pinkie or element == Blitz or 
                       element == Pandemonium or element == Harbrinder or element == A60 or element == Painter or
                       element == Keycard or element == Line5 or element == Line5_Border or element == EspText or
                       element == ESPitems or element == Angler_Bg or element == Angler_IndBorder or element == Angler_Ind or element == Angler_Label or
                       element == Froger_Bg or element == Froger_IndBorder or element == Froger_Ind or element == Froger_Label or
                       element == Pinkie_Bg or element == Pinkie_IndBorder or element == Pinkie_Ind or element == Pinkie_Label or
                       element == Blitz_Bg or element == Blitz_IndBorder or element == Blitz_Ind or element == Blitz_Label or
                       element == Pandemonium_Bg or element == Pandemonium_IndBorder or element == Pandemonium_Ind or element == Pandemonium_Label or
                       element == Harbrinder_Bg or element == Harbrinder_IndBorder or element == Harbrinder_Ind or element == Harbrinder_Label or
                       element == A60_Bg or element == A60_IndBorder or element == A60_Ind or element == A60_Label or
                       element == Painter_Bg or element == Painter_IndBorder or element == Painter_Ind or element == Painter_Label or
                       element == Keycard_Bg or element == Keycard_IndBorder or element == Keycard_Ind or element == Keycard_Label or
                       element == AutoHideGUI or element == AutoHideGUI_Inner or element == AutoHideGUI_Label or
                       element == WatermarkButton or element == WatermarkButton_Inner or element == WatermarkButton_Label then
                    element.Visible = false
                else
                    element.Visible = true
                end
            else
                element.Visible = false
            end
        end
    end
    print("[GUI] " .. (Settings.guiVisible and "SHOWN" or "HIDDEN"))
end

-- F1 Key Handler
spawn(function()
    local lastF1 = false
    while true do
        if isrbxactive() then
            local f1Pressed = iskeypressed(112)
            if f1Pressed and not lastF1 then
                ToggleGUI()
            end
            lastF1 = f1Pressed
        else
            lastF1 = false
        end
        wait(0.05)
    end
end)

-- Tab System Functions
local function Tab_TabHomeContainer1_SetVisible(visible)
    if not Settings.guiVisible then return end
    if TabHomeContainer1 then TabHomeContainer1.Visible = visible end
    if WorkGames1 then WorkGames1.Visible = visible end
    if PressureWORK then PressureWORK.Visible = visible end
end

local function Tab_TabVisualsContainer_SetVisible(visible)
    if not Settings.guiVisible then return end
    if TabVisualsContainer then TabVisualsContainer.Visible = visible end
    if Angler then
        Angler.Visible = visible
        if Angler_Bg then Angler_Bg.Visible = visible end
        if Angler_IndBorder then Angler_IndBorder.Visible = visible end
        if Angler_Ind then Angler_Ind.Visible = visible end
        if Angler_Label then Angler_Label.Visible = visible end
    end
    if Froger then
        Froger.Visible = visible
        if Froger_Bg then Froger_Bg.Visible = visible end
        if Froger_IndBorder then Froger_IndBorder.Visible = visible end
        if Froger_Ind then Froger_Ind.Visible = visible end
        if Froger_Label then Froger_Label.Visible = visible end
    end
    if Pinkie then
        Pinkie.Visible = visible
        if Pinkie_Bg then Pinkie_Bg.Visible = visible end
        if Pinkie_IndBorder then Pinkie_IndBorder.Visible = visible end
        if Pinkie_Ind then Pinkie_Ind.Visible = visible end
        if Pinkie_Label then Pinkie_Label.Visible = visible end
    end
    if Blitz then
        Blitz.Visible = visible
        if Blitz_Bg then Blitz_Bg.Visible = visible end
        if Blitz_IndBorder then Blitz_IndBorder.Visible = visible end
        if Blitz_Ind then Blitz_Ind.Visible = visible end
        if Blitz_Label then Blitz_Label.Visible = visible end
    end
    if Pandemonium then
        Pandemonium.Visible = visible
        if Pandemonium_Bg then Pandemonium_Bg.Visible = visible end
        if Pandemonium_IndBorder then Pandemonium_IndBorder.Visible = visible end
        if Pandemonium_Ind then Pandemonium_Ind.Visible = visible end
        if Pandemonium_Label then Pandemonium_Label.Visible = visible end
    end
    if Harbrinder then
        Harbrinder.Visible = visible
        if Harbrinder_Bg then Harbrinder_Bg.Visible = visible end
        if Harbrinder_IndBorder then Harbrinder_IndBorder.Visible = visible end
        if Harbrinder_Ind then Harbrinder_Ind.Visible = visible end
        if Harbrinder_Label then Harbrinder_Label.Visible = visible end
    end
    if A60 then
        A60.Visible = visible
        if A60_Bg then A60_Bg.Visible = visible end
        if A60_IndBorder then A60_IndBorder.Visible = visible end
        if A60_Ind then A60_Ind.Visible = visible end
        if A60_Label then A60_Label.Visible = visible end
    end
    if Painter then
        Painter.Visible = visible
        if Painter_Bg then Painter_Bg.Visible = visible end
        if Painter_IndBorder then Painter_IndBorder.Visible = visible end
        if Painter_Ind then Painter_Ind.Visible = visible end
        if Painter_Label then Painter_Label.Visible = visible end
    end
    if Keycard then
        Keycard.Visible = visible
        if Keycard_Bg then Keycard_Bg.Visible = visible end
        if Keycard_IndBorder then Keycard_IndBorder.Visible = visible end
        if Keycard_Ind then Keycard_Ind.Visible = visible end
        if Keycard_Label then Keycard_Label.Visible = visible end
    end
    if Line5 then
        Line5.Visible = visible
        if Line5_Border then Line5_Border.Visible = visible end
    end
    if EspText then EspText.Visible = visible end
    if ESPitems then ESPitems.Visible = visible end
end

local function Tab_MiscTABcontainer1_SetVisible(visible)
    if not Settings.guiVisible then return end
    if MiscTABcontainer1 then MiscTABcontainer1.Visible = visible end
    if AutoHideGUI then
        AutoHideGUI.Visible = visible
        if AutoHideGUI_Label then AutoHideGUI_Label.Visible = visible end
        if AutoHideGUI_Inner then AutoHideGUI_Inner.Visible = visible and AutoHideGUI_IsChecked end
    end
    if WatermarkButton then
        WatermarkButton.Visible = visible
        if WatermarkButton_Label then WatermarkButton_Label.Visible = visible end
        if WatermarkButton_Inner then WatermarkButton_Inner.Visible = visible and WatermarkButton_IsChecked end
    end
end

-- Input Handling
local dragging = nil
local dragStart = nil
local startPos = nil
local knobStartPos = nil
local lastMouse1 = false

while true do
    wait(0.01)
    if isrbxactive() then
        local mouse1 = ismouse1pressed()
        local mPos = Vector2.new(Mouse.X, Mouse.Y)

        if mouse1 and not lastMouse1 then
            if HOMEtabB.Visible and mPos.X >= HOMEtabB.Position.X and mPos.X <= HOMEtabB.Position.X + HOMEtabB.Size.X and
               mPos.Y >= HOMEtabB.Position.Y and mPos.Y <= HOMEtabB.Position.Y + HOMEtabB.Size.Y then
                Tab_TabVisualsContainer_SetVisible(false)
                Tab_MiscTABcontainer1_SetVisible(false)
                Tab_TabHomeContainer1_SetVisible(not TabHomeContainer1.Visible)
            end
            
            if TabVisualsB.Visible and mPos.X >= TabVisualsB.Position.X and mPos.X <= TabVisualsB.Position.X + TabVisualsB.Size.X and
               mPos.Y >= TabVisualsB.Position.Y and mPos.Y <= TabVisualsB.Position.Y + TabVisualsB.Size.Y then
                Tab_TabHomeContainer1_SetVisible(false)
                Tab_MiscTABcontainer1_SetVisible(false)
                Tab_TabVisualsContainer_SetVisible(not TabVisualsContainer.Visible)
            end
            
            if MiscTAB.Visible and mPos.X >= MiscTAB.Position.X and mPos.X <= MiscTAB.Position.X + MiscTAB.Size.X and
               mPos.Y >= MiscTAB.Position.Y and mPos.Y <= MiscTAB.Position.Y + MiscTAB.Size.Y then
                Tab_TabHomeContainer1_SetVisible(false)
                Tab_TabVisualsContainer_SetVisible(false)
                Tab_MiscTABcontainer1_SetVisible(not MiscTABcontainer1.Visible)
            end
            
            if Angler_Label.Visible and mPos.X >= Angler.Position.X and mPos.X <= Angler.Position.X + Angler.Size.X and
               mPos.Y >= Angler.Position.Y and mPos.Y <= Angler.Position.Y + Angler.Size.Y then
                Angler_IsChecked = not Angler_IsChecked
                Settings.notificationsEnabled.Angler = Angler_IsChecked
                if Angler_IsChecked then
                    Angler_IndBorder.Position = Angler.Position + Vector2.new(21, 1)
                    Angler_Ind.Position = Angler.Position + Vector2.new(21, 1)
                else
                    Angler_IndBorder.Position = Angler.Position + Vector2.new(1, 1)
                    Angler_Ind.Position = Angler.Position + Vector2.new(1, 1)
                end
                print("[Angler Notification] " .. (Angler_IsChecked and "ON" or "OFF"))
            end
            
            if Froger_Label.Visible and mPos.X >= Froger.Position.X and mPos.X <= Froger.Position.X + Froger.Size.X and
               mPos.Y >= Froger.Position.Y and mPos.Y <= Froger.Position.Y + Froger.Size.Y then
                Froger_IsChecked = not Froger_IsChecked
                Settings.notificationsEnabled.Froger = Froger_IsChecked
                if Froger_IsChecked then
                    Froger_IndBorder.Position = Froger.Position + Vector2.new(21, 1)
                    Froger_Ind.Position = Froger.Position + Vector2.new(21, 1)
                else
                    Froger_IndBorder.Position = Froger.Position + Vector2.new(1, 1)
                    Froger_Ind.Position = Froger.Position + Vector2.new(1, 1)
                end
                print("[Froger Notification] " .. (Froger_IsChecked and "ON" or "OFF"))
            end
            
            if Pinkie_Label.Visible and mPos.X >= Pinkie.Position.X and mPos.X <= Pinkie.Position.X + Pinkie.Size.X and
               mPos.Y >= Pinkie.Position.Y and mPos.Y <= Pinkie.Position.Y + Pinkie.Size.Y then
                Pinkie_IsChecked = not Pinkie_IsChecked
                Settings.notificationsEnabled.Pinkie = Pinkie_IsChecked
                if Pinkie_IsChecked then
                    Pinkie_IndBorder.Position = Pinkie.Position + Vector2.new(21, 1)
                    Pinkie_Ind.Position = Pinkie.Position + Vector2.new(21, 1)
                else
                    Pinkie_IndBorder.Position = Pinkie.Position + Vector2.new(1, 1)
                    Pinkie_Ind.Position = Pinkie.Position + Vector2.new(1, 1)
                end
                print("[Pinkie Notification] " .. (Pinkie_IsChecked and "ON" or "OFF"))
            end
            
            if Blitz_Label.Visible and mPos.X >= Blitz.Position.X and mPos.X <= Blitz.Position.X + Blitz.Size.X and
               mPos.Y >= Blitz.Position.Y and mPos.Y <= Blitz.Position.Y + Blitz.Size.Y then
                Blitz_IsChecked = not Blitz_IsChecked
                Settings.notificationsEnabled.Blitz = Blitz_IsChecked
                if Blitz_IsChecked then
                    Blitz_IndBorder.Position = Blitz.Position + Vector2.new(21, 1)
                    Blitz_Ind.Position = Blitz.Position + Vector2.new(21, 1)
                else
                    Blitz_IndBorder.Position = Blitz.Position + Vector2.new(1, 1)
                    Blitz_Ind.Position = Blitz.Position + Vector2.new(1, 1)
                end
                print("[Blitz Notification] " .. (Blitz_IsChecked and "ON" or "OFF"))
            end
            
            if Pandemonium_Label.Visible and mPos.X >= Pandemonium.Position.X and mPos.X <= Pandemonium.Position.X + Pandemonium.Size.X and
               mPos.Y >= Pandemonium.Position.Y and mPos.Y <= Pandemonium.Position.Y + Pandemonium.Size.Y then
                Pandemonium_IsChecked = not Pandemonium_IsChecked
                Settings.notificationsEnabled.Pandemonium = Pandemonium_IsChecked
                if Pandemonium_IsChecked then
                    Pandemonium_IndBorder.Position = Pandemonium.Position + Vector2.new(21, 1)
                    Pandemonium_Ind.Position = Pandemonium.Position + Vector2.new(21, 1)
                else
                    Pandemonium_IndBorder.Position = Pandemonium.Position + Vector2.new(1, 1)
                    Pandemonium_Ind.Position = Pandemonium.Position + Vector2.new(1, 1)
                end
                print("[Pandemonium Notification] " .. (Pandemonium_IsChecked and "ON" or "OFF"))
            end
            
            if Harbrinder_Label.Visible and mPos.X >= Harbrinder.Position.X and mPos.X <= Harbrinder.Position.X + Harbrinder.Size.X and
               mPos.Y >= Harbrinder.Position.Y and mPos.Y <= Harbrinder.Position.Y + Harbrinder.Size.Y then
                Harbrinder_IsChecked = not Harbrinder_IsChecked
                Settings.notificationsEnabled.Chainsmoker = Harbrinder_IsChecked
                if Harbrinder_IsChecked then
                    Harbrinder_IndBorder.Position = Harbrinder.Position + Vector2.new(21, 1)
                    Harbrinder_Ind.Position = Harbrinder.Position + Vector2.new(21, 1)
                else
                    Harbrinder_IndBorder.Position = Harbrinder.Position + Vector2.new(1, 1)
                    Harbrinder_Ind.Position = Harbrinder.Position + Vector2.new(1, 1)
                end
                print("[Chainsmoker Notification] " .. (Harbrinder_IsChecked and "ON" or "OFF"))
            end
            
            if A60_Label.Visible and mPos.X >= A60.Position.X and mPos.X <= A60.Position.X + A60.Size.X and
               mPos.Y >= A60.Position.Y and mPos.Y <= A60.Position.Y + A60.Size.Y then
                A60_IsChecked = not A60_IsChecked
                Settings.notificationsEnabled["A-60"] = A60_IsChecked
                if A60_IsChecked then
                    A60_IndBorder.Position = A60.Position + Vector2.new(21, 1)
                    A60_Ind.Position = A60.Position + Vector2.new(21, 1)
                else
                    A60_IndBorder.Position = A60.Position + Vector2.new(1, 1)
                    A60_Ind.Position = A60.Position + Vector2.new(1, 1)
                end
                print("[A-60 Notification] " .. (A60_IsChecked and "ON" or "OFF"))
            end
            
            if Painter_Label.Visible and mPos.X >= Painter.Position.X and mPos.X <= Painter.Position.X + Painter.Size.X and
               mPos.Y >= Painter.Position.Y and mPos.Y <= Painter.Position.Y + Painter.Size.Y then
                Painter_IsChecked = not Painter_IsChecked
                Settings.notificationsEnabled.Painter = Painter_IsChecked
                if Painter_IsChecked then
                    Painter_IndBorder.Position = Painter.Position + Vector2.new(21, 1)
                    Painter_Ind.Position = Painter.Position + Vector2.new(21, 1)
                else
                    Painter_IndBorder.Position = Painter.Position + Vector2.new(1, 1)
                    Painter_Ind.Position = Painter.Position + Vector2.new(1, 1)
                end
                print("[Painter Notification] " .. (Painter_IsChecked and "ON" or "OFF"))
            end
            
            if Keycard_Label.Visible and mPos.X >= Keycard.Position.X and mPos.X <= Keycard.Position.X + Keycard.Size.X and
               mPos.Y >= Keycard.Position.Y and mPos.Y <= Keycard.Position.Y + Keycard.Size.Y then
                Keycard_IsChecked = not Keycard_IsChecked
                if Keycard_IsChecked then
                    Keycard_IndBorder.Position = Keycard.Position + Vector2.new(21, 1)
                    Keycard_Ind.Position = Keycard.Position + Vector2.new(21, 1)
                    StartKeycardESP()
                else
                    Keycard_IndBorder.Position = Keycard.Position + Vector2.new(1, 1)
                    Keycard_Ind.Position = Keycard.Position + Vector2.new(1, 1)
                    CleanupKeycardESP()
                end
            end
            
            if AutoHideGUI.Visible and mPos.X >= AutoHideGUI.Position.X and mPos.X <= AutoHideGUI.Position.X + AutoHideGUI.Size.X and
               mPos.Y >= AutoHideGUI.Position.Y and mPos.Y <= AutoHideGUI.Position.Y + AutoHideGUI.Size.Y then
                AutoHideGUI_IsChecked = not AutoHideGUI_IsChecked
                AutoHideGUI_Inner.Visible = AutoHideGUI_IsChecked
                AutoHideSystem.enabled = AutoHideGUI_IsChecked
                Settings.autoHideEnabled = AutoHideGUI_IsChecked
                print("[AutoHide] " .. (AutoHideGUI_IsChecked and "ENABLED" or "DISABLED"))
            end
            
            if WatermarkButton.Visible and mPos.X >= WatermarkButton.Position.X and mPos.X <= WatermarkButton.Position.X + WatermarkButton.Size.X and
               mPos.Y >= WatermarkButton.Position.Y and mPos.Y <= WatermarkButton.Position.Y + WatermarkButton.Size.Y then
                WatermarkButton_IsChecked = not WatermarkButton_IsChecked
                WatermarkButton_Inner.Visible = WatermarkButton_IsChecked
                WatermarkSystem.enabled = WatermarkButton_IsChecked
                WatermarkSystem:SetVisible(WatermarkButton_IsChecked)
                Settings.watermarkEnabled = WatermarkButton_IsChecked
                print("[Watermark] " .. (WatermarkButton_IsChecked and "ENABLED" or "DISABLED"))
            end
            
            if MainUIsystem.Visible and mPos.X >= MainUIsystem.Position.X and mPos.X <= MainUIsystem.Position.X + MainUIsystem.Size.X and
               mPos.Y >= MainUIsystem.Position.Y and mPos.Y <= MainUIsystem.Position.Y + 43 then
                dragging = MainUIsystem
                dragStart = mPos
                startPos = MainUIsystem.Position
            end
        end

        if not mouse1 and lastMouse1 then
            dragging = nil
        end

        if dragging and mouse1 then
            local delta = mPos - dragStart
            dragging.Position = startPos + delta
            if dragging == MainUIsystem then
                MainUIsystem_Border.Position = dragging.Position
                Line.Position = dragging.Position + Vector2.new(188.5, 0)
                Line_Border.Position = Line.Position
                Line1.Position = dragging.Position + Vector2.new(0, 43)
                Line1_Border.Position = Line1.Position
                RATHUBlogo.Position = dragging.Position + Vector2.new(71.5, 15)
                Line2.Position = dragging.Position + Vector2.new(191.5, 43)
                Line2_Border.Position = Line2.Position
                HOMEtabB.Position = dragging.Position + Vector2.new(9.5, 51)
                HOMEtabB_Text.Position = HOMEtabB.Position + Vector2.new(172/2, 43/2)
